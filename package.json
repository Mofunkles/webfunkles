{
  "name": "webfunkles",
  "version": "1.0.0",
  "description": "Webpack with Eleventy boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "build": "npm-run-all clean prod:*",
    "clean": "rimraf src/html",
    "dev:eleventy": "cross-env ELEVENTY_ENV=development eleventy --watch --quiet",
    "dev:webpack": "webpack serve --config webpack.development.js",
    "prod:eleventy": "cross-env ELEVENTY_ENV=production eleventy",
    "prod:webpack": "webpack --config webpack.production.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/Mofunkles/webfunkles.git"
  },
  "author": "Mofunkles",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mofunkles/webfunkles/issues"
  },
  "homepage": "https://github.com/Mofunkles/webfunkles#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.9",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.1",
    "html-loader": "^2.1.1",
    "html-webpack-plugin": "^5.2.0",
    "mini-css-extract-plugin": "^1.3.9",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^5.1.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^2.7.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
